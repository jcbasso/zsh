# Plugins
source ~/.zsh/zhooks/zhooks.plugin.zsh
source ~/.zsh/colorscripts/colorscript.plugin.zsh
source ~/.zsh/prompt/prompt.plugin.zsh
source ~/.zsh/vim-mode/vim-mode.plugin.zsh
source ~/.zsh/autosuggestions/autosuggestions.plugin.zsh
source ~/.zsh/colored-man/colored-man.plugin.zsh
source ~/.zsh/gcp/gcp.plugin.zsh
source ~/.zsh/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh # This needs to be at the end of the Plugins
source ~/.zsh/history/history.plugin.zsh # This needs to be below higlight script

# Aliases
source ~/.zsh/.zshalias
# Exports
source ~/.zsh/.zshexport
# Path
source ~/.zsh/.zshpath
# Environment Variables
source ~/.zsh/.zshenv
# Secrets
source ~/.zsh/.zshsecret
